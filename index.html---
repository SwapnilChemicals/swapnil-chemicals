      <!-- JS USED FROM HERE  --> 
      <script>
         // ✅ Set nav offset dynamically (anchor clicks perfect)
         (function () {
           function setNavOffset(){
             const nav = document.querySelector('.nav');
             if(!nav) return;
             const h = Math.ceil(nav.getBoundingClientRect().height);
             document.documentElement.style.setProperty('--nav-offset', (h + 16) + 'px');
           }
           window.addEventListener('load', setNavOffset);
           window.addEventListener('resize', setNavOffset);
           if (document.fonts && document.fonts.ready) {
             document.fonts.ready.then(setNavOffset).catch(()=>{});
           }
         })();
         
         // Products: filter + search
         const searchInput = document.getElementById('searchInput');
         const pills = Array.from(document.querySelectorAll('.pill'));
         const cards = Array.from(document.querySelectorAll('#productGrid .card'));
         
         let activeFilter = "all";
         
         function applyFilters(){
           const q = (searchInput.value || "").trim().toLowerCase();
           cards.forEach(card => {
             const name = (card.dataset.name || "").toLowerCase();
             const cat  = (card.dataset.category || "all").toLowerCase();
         
             const matchText = !q || name.includes(q);
             const matchCat  = (activeFilter === "all") || (cat === activeFilter);
         
             card.style.display = (matchText && matchCat) ? "" : "none";
           });
         }
         
         pills.forEach(p => {
           p.addEventListener('click', () => {
             pills.forEach(x => x.classList.remove('active'));
             p.classList.add('active');
             activeFilter = p.dataset.filter;
             applyFilters();
           });
         });
         
         searchInput.addEventListener('input', applyFilters);
         
         // Modal popup
         const modal = document.getElementById('modal');
         const modalImg = document.getElementById('modalImg');
         const modalTitle = document.getElementById('modalTitle');
         const modalDesc = document.getElementById('modalDesc');
         const modalPrice = document.getElementById('modalPrice');
         const modalClose = document.getElementById('modalClose');
         
         function openModal(card){
           const img = card.querySelector('img');
           modalImg.src = img ? img.src : "";
           modalImg.alt = img ? img.alt : "Product";
           modalTitle.textContent = card.querySelector('h3')?.textContent || "Product";
           modalDesc.textContent = card.dataset.desc || "Product details coming soon.";
           modalPrice.textContent = "Rs " + (card.dataset.price || "0");
         
           modal.classList.add('open');
           modal.setAttribute('aria-hidden', 'false');
           document.body.style.overflow = "hidden";
         }
         
         function closeModal(){
           modal.classList.remove('open');
           modal.setAttribute('aria-hidden', 'true');
           document.body.style.overflow = "";
         }
         
         cards.forEach(card => {
           card.addEventListener('click', () => openModal(card));
         });
         
         modalClose.addEventListener('click', closeModal);
         modal.addEventListener('click', (e) => {
           if(e.target === modal) closeModal();
         });
         window.addEventListener('keydown', (e) => {
           if(e.key === "Escape") closeModal();
         });
      </script>
